{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "no-underscore-dangle":  ["error", { "allow": ["_id"] }],
      "react/prop-types": 0,
      "no-shadow": "off",
    },
    "settings": {
        "import/resolver": {
          "node": {
            "paths": ["src", "/"]
          }
        }
      }
  }